                                                    <td class="text-end pe-0">@item.productName</td>
                                                    @*<td>@item.billDate.ToString("yyyy-mm-dd hh:ss")</td>*@
                                                    <td class="text-end pe-0">@item.createdDate.ToString("yyyy-mm-dd hh:ss")</td>
