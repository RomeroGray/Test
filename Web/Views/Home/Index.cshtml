                                                    @*<td>@item.orderId</td>*@
                                                    <td class="text-end pe-0">@item.productSold</td>
                                                    <td class="text-end pe-0">@item.priceTotal</td>
                                                    <td class="text-end pe-0">@item.orderStartDate.ToString("yyyy-mm-dd hh:ss")</td>
                                                    <td class="text-end pe-0">@item.orderStartDate.ToString("yyyy-mm-dd hh:ss")</td>
                                                    <td class="text-end pe-0">@item.orderStartDate.ToString("yyyy-mm-dd hh:ss")</td>
