                                                    <td class="text-end pe-0">@item.pickupZone</td>
                                                    <td class="text-end pe-0">@item.tierName.name</td>
                                                    <td class="text-end pe-0">@item.grossProfit</td>
                                                    <td class="text-end pe-0">@item.participantSold</td>
                                                    @foreach (IRAT.Inject.Model.MatchTable item2 in ViewBag.Lookup)
                                                    {
                                                        if (item2.ProductNumber > 0)
                                                        {
                                                            if (item.productNumber == item2.ProductNumber && item.pickupZone.Equals(item2.PickupZone) && item.tierName.name.Equals(item2.Tier.Description))
                                                            {
                                                                <td class="text-end pe-0">@item2.Supplier</td>
                                                                <td class="text-end pe-0">@(Convert.ToDecimal(item2.Cost).ToString("C"))</td>
                                                            }
                                                        }
                                                    }
